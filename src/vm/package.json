{
  "name": "@saus/vm",
  "version": "0.4.10",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && esbuild *.ts --out-extension:.js=.mjs --outdir=dist && tsc -p .",
    "dev": "tsc -p . --watch --sourcemap"
  },
  "dependencies": {
    "@utils": "workspace:@saus/utils@*",
    "builtin-modules": "^3.2.0",
    "debug": "^4.3.2",
    "es-module-lexer": "^1.0.5",
    "kleur": "^4.1.4",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@saus/utils": "workspace:*",
    "@types/babel__traverse": "^7.18.2"
  }
}
