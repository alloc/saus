{
  "name": "saus",
  "version": "0.4.10",
  "description": "Radically simple SSG/SSR powered by Vite",
  "repository": "alloc/saus",
  "author": "Alec Larson",
  "scripts": {
    "clean": "rimraf ./dist && git checkout HEAD dist",
    "build": "npm run clean && tsup && tsc -p . --emitDeclarationOnly && npm run link",
    "dev": "concurrently npm:dev:*",
    "dev:build": "tsup --watch --sourcemap",
    "dev:types": "tsc -p . --emitDeclarationOnly --watch",
    "link": "cd dist && ln -sf ../node_modules/@client ./client && ln -sf ../node_modules/@runtime ./runtime && ln -sf ../node_modules/@utils ./utils && ln -sf ../node_modules/@vm ./vm",
    "lint": "tsc -p . --noEmit && eslint . --ext .ts",
    "prepare": "npm run link"
  },
  "dependencies": {
    "@": "link:./core",
    "@ampproject/remapping": "^1.1.1",
    "@babel/code-frame": "^7.16.7",
    "@cush/exec": "^1.8.0",
    "@saus/client": "workspace:*",
    "@saus/runtime": "workspace:*",
    "@saus/utils": "workspace:*",
    "@saus/vm": "workspace:*",
    "cac": "^6.0.0",
    "convert-source-map": "^1.8.0",
    "debug": "^4.3.2",
    "ee-ts": "2.0.0-rc.6",
    "endent": "^2.1.0",
    "es-module-lexer": "1.0.5",
    "esbuild": "^0.14.15",
    "etag": "^1.8.1",
    "kleur": "^4.1.4",
    "misty": "^1.6.7",
    "mrmime": "^1.0.0",
    "recrawl-sync": "^2.2.1",
    "rollup": "^2.62.1",
    "shared-log": "^1.2.8",
    "source-map-support": "^0.5.21",
    "terser": "^5.10.0",
    "type-fest": "^2.13.0",
    "vite": "npm:@alloc/vite@2.6.3-3",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@client": "link:./node_modules/@saus/client/dist",
    "@rollup/pluginutils": "5.0.1",
    "@runtime": "link:./node_modules/@saus/runtime/dist",
    "@types/babel__code-frame": "^7.0.3",
    "@types/convert-source-map": "^1.5.2",
    "@types/estree": "^0.0.51",
    "@types/etag": "^1.8.1",
    "@types/glob-base": "^0.3.0",
    "@types/node": "^17.0.35",
    "@types/source-map-support": "^0.5.4",
    "@utils": "link:./node_modules/@saus/utils/dist",
    "@vm": "link:./node_modules/@saus/vm/dist",
    "arrify": "2.0.1",
    "base64-arraybuffer": "^1.0.2",
    "builtin-modules": "^3.2.0",
    "catch-exit": "^1.2.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "elaps": "^2.4.0",
    "escalade": "^3.1.1",
    "glob-base": "^0.3.0",
    "quick-lru": "6.1.1",
    "ts-debounce": "2.0.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
