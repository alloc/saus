{
  "name": "@saus/client",
  "version": "0.4.10",
  "scripts": {
    "clean": "rimraf dist && git checkout HEAD dist",
    "build": "npm run clean && esbuild *.ts **/*.ts --out-extension:.js=.mjs --outdir=dist && tsc -p .",
    "dev": "concurrently npm:dev:*",
    "dev:cjs": "tsc -p . --watch --sourcemap",
    "dev:mjs": "esbuild *.ts **/*.ts --out-extension:.js=.mjs --outdir=dist --watch --sourcemap=external"
  },
  "dependencies": {
    "@saus/runtime": "workspace:*",
    "@saus/utils": "workspace:*",
    "@saus/vm": "workspace:*"
  },
  "devDependencies": {
    "@runtime": "link:./node_modules/@saus/runtime/dist",
    "@utils": "link:./node_modules/@saus/utils/dist",
    "@vm": "link:./node_modules/@saus/vm/dist"
  }
}
