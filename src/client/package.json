{
  "name": "@saus/client",
  "version": "0.4.10",
  "scripts": {
    "clean": "rimraf dist && git checkout HEAD dist",
    "build": "npm run clean && esbuild *.ts **/*.ts --out-extension:.js=.mjs --outdir=dist && tsc -p .",
    "dev": "tsc -p . --watch --sourcemap"
  },
  "dependencies": {
    "@runtime": "workspace:@saus/runtime@*",
    "@utils": "workspace:@saus/utils@*",
    "@vm": "workspace:@saus/vm@*"
  },
  "devDependencies": {
    "@saus/runtime": "workspace:*",
    "@saus/utils": "workspace:*",
    "@saus/vm": "workspace:*"
  }
}
