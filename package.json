{
  "name": "saus",
  "version": "0.4.10",
  "description": "Radically simple SSG/SSR powered by Vite",
  "author": "Alec Larson",
  "license": "MIT",
  "repository": "alloc/saus",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "bin": "bin/saus",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./dist/index.js"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./babel": "./babel/index.js",
    "./bundle": "./bundle/index.js",
    "./client": {
      "node": "./client/index.cjs",
      "default": "./client/index.js"
    },
    "./deploy": "./deploy/index.js",
    "./env/*": "./env/*.d.ts",
    "./core": "./core/index.js",
    "./http": "./http/index.js"
  },
  "files": [
    "src",
    "dist",
    "babel",
    "bundle",
    "client",
    "core",
    "deploy",
    "env",
    "http"
  ],
  "scripts": {
    "build": "pnpm -r --no-bail --filter '@saus/*' build",
    "dev": "pnpm build && pnpm -r --no-bail --filter '@saus/*' --parallel dev",
    "prepublishOnly": "yarn build",
    "test": "vitest"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@": "link:./src/core",
    "@ampproject/remapping": "^2.1.2",
    "@babel/code-frame": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-typescript": "^7.14.5",
    "@http": "workspace:@saus/http@*",
    "@runtime": "workspace:@saus/runtime@*",
    "@saus/deploy-utils": "workspace:^0.1.0",
    "@saus/html": "workspace:^0.1.0",
    "@utils": "workspace:@saus/utils@*",
    "@vm": "workspace:@saus/vm@*",
    "ansi-to-html": "^0.7.2",
    "cac": "^6.0.0",
    "convert-source-map": "^1.8.0",
    "debug": "^4.3.2",
    "ee-ts": "2.0.0-rc.6",
    "endent": "^2.1.0",
    "es-module-lexer": "^0.9.3",
    "esbuild": "^0.14.15",
    "etag": "^1.8.1",
    "kleur": "^4.1.4",
    "magic-string": "^0.25.7",
    "misty": "^1.6.7",
    "mrmime": "^1.0.0",
    "ohash": "^0.1.5",
    "recrawl-sync": "^2.2.1",
    "rollup": "^2.62.1",
    "shared-log": "^1.2.8",
    "source-map-support": "^0.5.21",
    "strict-event-emitter-types": "^2.0.0",
    "terser": "^5.10.0",
    "tinypool": "^0.1.1",
    "type-fest": "^2.13.0",
    "vite": "npm:@alloc/vite@2.6.3-3",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@alloc/fast-rimraf": "^1.0.8",
    "@alloc/prettier-config": "^1.0.0",
    "@babel/helper-annotate-as-pure": "^7.15.4",
    "@cush/exec": "^1.8.0",
    "@cush/relative": "^1.0.0",
    "@rollup/pluginutils": "^4.1.2",
    "@types/babel__code-frame": "^7.0.3",
    "@types/babel__core": "^7.1.16",
    "@types/babel__traverse": "^7.18.2",
    "@types/convert-source-map": "^1.5.2",
    "@types/debug": "^4.1.7",
    "@types/estree": "^0.0.51",
    "@types/etag": "^1.8.1",
    "@types/glob-base": "^0.3.0",
    "@types/node": "^17.0.35",
    "@types/physical-cpu-count": "^2.0.0",
    "@types/prettier": "^2.6.3",
    "@types/source-map-support": "^0.5.4",
    "@types/strip-comments": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "arrify": "^3.0.0",
    "base64-arraybuffer": "^1.0.2",
    "builtin-modules": "^3.2.0",
    "callsites": "^3.0.0",
    "catch-exit": "^1.2.2",
    "chokidar": "^3.5.3",
    "elaps": "^2.4.0",
    "escalade": "^3.1.1",
    "eslint": "^8.24.0",
    "glob-base": "^0.3.0",
    "is-plain-object": "^5.0.0",
    "prettier": "^2.0.5",
    "quick-lru": "^6.1.1",
    "rollup-plugin-dts": "^4.1.0",
    "source-map": "0.6.1",
    "strip-comments": "^2.0.1",
    "ts-debounce": "2.0.0",
    "tsup": "^5.12.8",
    "typescript": "4.8.0-beta",
    "vitest": "*"
  },
  "prettier": "@alloc/prettier-config",
  "pnpm": {
    "overrides": {
      "@babel/types": "7.18.8"
    }
  }
}
